<h1>This are all the Restaurants</h1>

<%= link_to 'Add New Restaurant', new_restaurant_path %>

<% @restaurants.each do |restaurant| %>
  <p><%= link_to restaurant.name, restaurant_path(restaurant) %> - <%= restaurant.address %> - <%= restaurant.category %> - <%= link_to "Add Review", new_restaurant_review_path(restaurant) %></p>
<% end %>
